<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloCordova</name>
    <description />
    <author email="dev@cordova.apache.org" href="http://cordova.io" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
		<access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <plugin name="cordova-plugin-remote-injection" spec="~0.4.0" />
    <content src="http://localhost:8000/" />
		<!-- <plugin name="cordova-plugin-wkwebview-engine" spec="https://github.com/ccorcos/cordova-plugin-wkwebview-engine#fddccc4dce724b51aea662221150501a672e0382" /> -->
    <plugin name="cordova-plugin-wkwebview-engine" spec="~1.1.3" />
		<preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <!-- <plugin name="cordova-plugin-keyboard" spec="https://github.com/ccorcos/cordova-plugin-keyboard#c38186f10f35261e4b8234d530efb2a27a10deea" /> -->
    <plugin name="cordova-plugin-keyboard" spec="~1.1.5" />
    <preference name="KeyboardShrinksView" value="true" />
</widget>
